generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id                String   @id @default(cuid())
  firstName         String
  lastName          String
  gender            String
  caste             String
  dateOfBirth       DateTime
  timeOfBirth       String
  city              String
  country           String
  maritalStatus     String
  mobileNumber      String
  email             String
  profilePictureUrl String
  passwordHash      String
  serialNumber      String   @unique
}
